<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spring.bugs.server.controller.tester.RemoveBugController">
    <children>
        <TableView fx:id="bugsTableView" layoutX="26.0" layoutY="22.0" prefHeight="200.0" prefWidth="550.0">
            <columns>
                <TableColumn fx:id="titleColumn" prefWidth="75.0" text="Title"/>
                <TableColumn fx:id="registererColumn" prefWidth="88.800048828125" text="Registerer"/>
                <TableColumn fx:id="registeredColumn" prefWidth="96.7999267578125" text="Registered"/>
                <TableColumn fx:id="statusColumn" prefWidth="75.20001220703125" text="Status"/>
                <TableColumn fx:id="resolverColumn" prefWidth="104.79998779296875" text="Resolver"/>
                <TableColumn fx:id="resolvedColumn" prefWidth="111.199951171875" text="Resolved"/>
            </columns>
        </TableView>
        <TextArea fx:id="descriptionArea" layoutX="233.0" layoutY="236.0" prefHeight="102.0" prefWidth="278.0"/>
        <Label layoutX="119.0" layoutY="243.0" text="Description">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Button fx:id="deleteBtn" layoutX="233.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleDelete"
                prefHeight="26.0" prefWidth="124.0" text="Delete"/>
    </children>
</AnchorPane>
